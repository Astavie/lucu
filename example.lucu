effect write {
	fun write(s str);
}

effect log {
	fun log(s str);
}

# effect group
effect sysio = (stdout write) (stderr write);

fun main() / sysio {

	try {

		log("Hello, World!");

	} with log {

		fun log(s str) {
			stdout::write("\"");
			stdout::write(s);
			stdout::write("\"\n");
		}

	};

}
